{
  "name": "@pg-nano/libpq",
  "version": "1.8.13",
  "description": "Low-level native bindings to PostgreSQL libpq",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": ["postgres", "libpq"],
  "files": ["src", "binding.gyp", "index.d.ts", "index.js"],
  "repository": {
    "type": "git",
    "url": "git://github.com/pg-nano/node-libpq.git"
  },
  "scripts": {
    "clean": "node-gyp clean",
    "postinstall": "[ -d .git ] && pnpm -s clangd:init || true",
    "pretest": "pnpm -s prepare",
    "test": "node_modules/.bin/mocha",
    "clangd:init": "cd build && compiledb make -f binding.Makefile -n"
  },
  "author": "Brian M. Carlson",
  "license": "MIT",
  "dependencies": {
    "bindings": "1.5.0",
    "nan": "2.19.0"
  },
  "devDependencies": {
    "buffer-from": "^1.1.1",
    "mocha": "10.4.0"
  },
  "prettier": {
    "singleQuote": true
  }
}
